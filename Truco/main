    numero1 = aux[0]
    palo1 = aux [1]
    aux = auxiliarCarta2.split (" ")
    numero2 = aux[0]
    palo2 = aux [1]
    aux = auxiliarCarta3.split (" ")
    numero3 = aux[0]
    palo3 = aux [1]
    print (palo1,palo2,palo3)
    if (palo1 == palo2 or palo1 == palo3 or palo2 == palo3):
        if (palo1 == palo2):
            if (numero1 != 10 or numero1 != 11 or numero1 != 12 and numero2 != 10 or numero2 != 11 or numero2 != 12):
                envido = int(numero1) + int(numero2) + 20
                print (envido)

            elif (numero1 == 10 or numero1 == 11 or numero1 == 12 and numero2 != 10 or numero2 != 11 or numero2 != 12):
                envido = int(numero2) + 20
                print (envido)

            elif (numero1 == 10 or numero1 == 11 or numero1 == 12 and numero2 == 10 or numero2 == 11 or numero2 == 12):
                envido = 20
        elif (palo2 == palo3):
            if (numero3 != 10 or numero3 != 11 or numero3 != 12 and numero2 != 10 or numero2 != 11 or numero2 != 12):
                envido = int(numero3) + int(numero2) + 20
                print (envido)

            elif (numero3 == 10 or numero3 == 11 or numero3 == 12 and numero2 != 10 or numero2 != 11 or numero2 != 12):
                envido = int(numero2) + 20
                print (envido)

            elif (numero3 == 10 or numero3 == 11 or numero3 == 12 and numero2 == 10 or numero2 == 11 or numero2 == 12):
                envido = 20
        elif (palo1 == palo3):
            if (numero1 != 10 or numero1 != 11 or numero1 != 12 and numero3 != 10 or numero3 != 11 or numero3 != 12):
