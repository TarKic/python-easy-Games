import random 
def cartas ():
    cantidad = 0
    mano = []
    mano2 = []
    baraja = [1,2,3,4,5,6,7,10,11,12]
    palo = ['Oro','Basto','Espada', 'Copa']
    while cantidad != 3:
        carta = str(baraja[random.randint (0,9)]) + (" ") + palo [random.randint (0,3)]
        cantidad += 1
        mano.append (carta)
        mano2.append (carta)
    if (mano [0] == mano [1]):
        del mano [0]
        mano.append (carta)
    elif  (mano [1] == mano [2]):
        del mano[1]
        mano.append (carta)
    else:
        del mano [2]
        mano.append (carta)
   
    return (mano)

def controlaCartas (mano1,mano2):
    if (mano1 [0] == mano2[0] or mano1 [0] == mano2[1] or mano1 [0] == mano2[2]):
        del mano1[0]
        mano1.append (cartas())
    elif (mano1 [1] == mano2[0] or mano1 [1] == mano2[1] or mano1 [1] == mano2[2]):
        del mano1[1]
        mano1.append (cartas())
    elif(mano1 [2] == mano2[0] or mano1 [2] == mano2[1] or mano1 [2] == mano2[2]):
        del mano1[2]
        mano1.append (cartas())
    return mano1
 
def envido (mano1,mano2): #Revisar el envido... 
    auxiliarCarta1 = mano1[0]
    auxiliarCarta2 = mano1[1]
    auxiliarCarta3 = mano1[2]
