                envido = 20

                print (envido)
            
    
cantidadJugadores = int (input ("Ingrese la cantidad de jugadores: "))
while cantidadJugadores != 2:
    cantidadJugadores = int (input ("Ingrese la cantidad de jugadores: "))

manoJugador1 = cartas ()
manoJugador2 = cartas ()
manoJugador1 = controlaCartas (manoJugador1,manoJugador2)

print (manoJugador1, manoJugador2)
